# Install / upgrade package
dpkg -i test.deb

# Remove a package including config files
dpkg -P test.deb

# List installed packages with versions and details
dpkg -I

# Check if a package is installed
dpkg -s test.deb | grep Status

# List files installed by a package
dpkg-query -L <pkg-name>

# List files a package would install
dpkg-deb -c <pkg-name.deb>

